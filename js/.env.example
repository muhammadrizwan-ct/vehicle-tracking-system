# Server Configuration
PORT=3000
NODE_ENV=development

# Google Sheets API
SPREADSHEET_ID=your_google_sheet_id_here
GOOGLE_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json

# JWT Secret
